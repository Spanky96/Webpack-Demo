*{
    margin: 0;
    padding: 0;
}
.amap-info-contentContainer {
    padding-bottom: 40px;
    padding-left: 150px;
}
.amap-info-contentContainer::before {
    content: "";
    display: block;
    border-width: 23px;
    position: absolute;
    bottom: -2px;
    left: 227px;
    border-style: solid dashed dashed;
    border-color: #fff transparent transparent;
    font-size: 0;
    line-height: 0;
}
.amap-info-contentContainer::after {
    content: "";
    display: block;
    border-width: 20px;
    position: absolute;
    left: 230px;
    border-style: solid dashed dashed;
    bottom: 5px;
    border-color: #068764 transparent transparent;
    font-size: 0;
    line-height: 0;
}
.amap-info-outer{width:490px;height:600px} .bottom-center.amap-info-contentContainer{padding-bottom:40px;padding-left:150px} .bottom-center.amap-info-contentContainer:before{content:"";display:block;position:absolute;bottom:-2px;left:227px;border:23px dashed transparent;border-top:23px solid #fff;font-size:0;line-height:0} .bottom-center.amap-info-contentContainer:after{content:"";display:block;position:absolute;left:230px;bottom:5px;border:20px dashed transparent;border-top:20px solid #068764;font-size:0;line-height:0} .bottom-center .amap-info-sharp{display:none} .amap-info-outer{background:-webkit-gradient(linear,left top,left bottom,from(#068764),to(#068764));width:320px;height:200px;padding:10px;border:4px solid #fff;border-radius:10px} .amap-info-outer .amap-info-close{font-size:34px;color:#fcfff3;top:-10px;right:10px!important;position: absolute;text-decoration: none;} .amap-info-outer::-webkit-scrollbar{width:6px;height:3px} .amap-info-outer::-webkit-scrollbar-button, .amap-info-outer ::-webkit-scrollbar-track{display:none} .amap-info-outer::-webkit-scrollbar-track-piece{background:#068764} .amap-info-outer::-webkit-scrollbar-thumb{background:rgba(0,255,221,.32);border-radius:10px}
.local-zb {
    width: 700px;
    height: 40px;
    position: absolute;
    left: 24px;
    top: 24px;
    color: #fff;
    z-index: 1
}

.local-zb:before {
    content: "";
    position: absolute;
    background: url(data:image/png;base64,) no-repeat 0 5px;
    width: 40px;
    height: 40px
}

.local-zb ul {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    list-style: none;
    font-size: 24px;
    line-height: 40px;
    margin-left: 30px
}

.local-zb ul li:not(:last-child) {
    cursor: pointer;
}

.local-zb ul li:not(:last-child):after {
    content: ">"
}

.local-zb ul li:last-child {
    color: #36d4dd
}